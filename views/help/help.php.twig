<div id="helpModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span class="fa fa-times" aria-hidden="true"></span><span class="sr-only">{%trans%}help.close{%endtrans%}</span></button>
                <h4 class="modal-title">{%trans%}help.codetest{%endtrans%}</h4>
            </div>
            <div class="modal-body">
                {% set helpFilename = 'help/' ~  help %}
                {{ include(helpFilename) }}
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">{%trans%}help.close{%endtrans%}</button>
            </div>
        </div>

    </div>
</div>
